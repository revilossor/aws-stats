{
  "name": "aws-stats",
  "version": "0.0.3",
  "description": "an express router that exposes an api to get cloudmetrics stats from aws",
  "main": "main.js",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "npm test",
    "lint": "npx eslint .",
    "test": "jest",
    "start": "npx nodemon app/main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/revilossor/aws-stats.git"
  },
  "author": "oliver.ross@hotmail.com",
  "license": "SEE LICENCE IN LICENCE",
  "dependencies": {
    "aws-sdk": "^2.83.0",
    "express": "^4.15.3"
  },
  "devDependencies": {
    "eslint": "^4.2.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
