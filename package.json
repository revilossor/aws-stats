{
  "name": "aws-stats",
  "version": "0.0.0",
  "description": "an express router that exposes an api to get cloudmetrics stats from aws",
  "main": "main.js",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "npm test",
    "lint": "npx eslint .",
    "test": "jest",
    "dev": "npx nodemon app/main"
  },
  "repository": {
    "type": "git",
    "url": ".."
  },
  "author": "oliver.ross@hotmail.com",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.83.0",
    "express": "^4.15.3"
  },
  "devDependencies": {
    "eslint": "^4.2.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "/dist/"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
